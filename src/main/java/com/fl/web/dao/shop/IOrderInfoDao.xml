<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.fl.web.dao.shop.IOrderInfoDao">

    <resultMap type="TOrderInfo" id="orderMap">
        <result property="id" column="id"/>
        <result property="deskId" column="desk_id"/>
        <result property="deskCode" column="desk_code"/>
        <result property="deskName" column="desk_name"/>
        <result property="custName" column="cust_name"/>
        <result property="custPhone" column="cust_phone"/>
        <result property="custFlag" column="cust_flag"/>
        <result property="goodsCode" column="goods_code"/>
        <result property="goodsName" column="goods_name"/>
        <result property="unit" column="unit"/>
        <result property="num" column="num"/>
        <result property="price" column="price"/>
        <result property="disPrice" column="dis_price"/>
        <result property="totalMoney" column="total_money"/>
        <result property="disMoney" column="dis_money"/>
        <result property="status" column="status"/>
        <result property="createBy" column="create_by"/>
        <result property="createDate" column="create_date"/>
        <result property="updateBy" column="update_by"/>
        <result property="updateDate" column="update_date"/>
    </resultMap>

    <insert id="saveOrderInfoBatch" parameterType="java.util.List">
        insert into order_info
        (id,desk_id,desk_code,desk_name,cust_name,cust_phone,cust_flag,goods_code,goods_name,unit,num,price,dis_price,total_money,dis_money,status,create_by,create_date)
        values
        <foreach collection="list" item="item" separator=",">
            (#{item.id},#{item.deskId},#{item.deskCode},#{item.deskName},#{item.custName},#{item.custPhone},#{item.custFlag},#{item.goodsCode},
            #{item.goodsName},#{item.unit},#{item.num},#{item.price},#{item.disPrice},#{item.totalMoney},#{item.disMoney},#{item.status},#{item.createBy},#{item.createDate})
        </foreach>
    </insert>

</mapper>